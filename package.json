{
  "name": "@rx-mind/angular-plugins",
  "description": "Reactive Plugins for Angular",
  "author": "Marko StanimiroviÄ‡ <markostanimirovic95@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && husky install",
    "nx": "nx",
    "start": "npm run start:data-cs-crud",
    "start:data-cs-crud": "concurrently \"npm run start:server:data-cs-crud\" \"ng serve --project=examples-data-cs-crud\"",
    "start:data-cs-search": "concurrently \"npm run start:server:data-cs-search\" \"ng serve --project=examples-data-cs-search\"",
    "start:data-cs-pagination": "concurrently \"npm run start:server:data-cs-pagination\" \"ng serve --project=examples-data-cs-pagination\"",
    "start:server:data-cs-crud": "json-server apps/examples/data-cs-crud/db.json --watch --delay 1000",
    "start:server:data-cs-search": "json-server apps/examples/data-cs-search/db.json --watch --delay 1000",
    "start:server:data-cs-pagination": "json-server apps/examples/data-cs-pagination/db.json --watch --delay 1000",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "test:coverage": "npm run test -- --codeCoverage",
    "lint": "nx run-many --target=lint --all",
    "e2e": "nx run-many --target=e2e --all",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,html,css,scss,md,json}": [
      "npm run format -- --uncommitted",
      "npm run affected:lint -- --uncommitted --fix --parallel"
    ]
  },
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/material": "^12.1.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@ngrx/component-store": "^12.0.0",
    "@nrwl/angular": "12.3.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular-eslint/eslint-plugin": "~12.0.0",
    "@angular-eslint/eslint-plugin-template": "~12.0.0",
    "@angular-eslint/template-parser": "~12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@nrwl/cli": "12.3.6",
    "@nrwl/cypress": "12.3.6",
    "@nrwl/eslint-plugin-nx": "12.3.6",
    "@nrwl/jest": "12.3.6",
    "@nrwl/linter": "12.3.6",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.3.6",
    "@nrwl/workspace": "12.3.6",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "concurrently": "^6.2.0",
    "cypress": "^7.3.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^6.0.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.4.0",
    "json-server": "^0.16.3",
    "lint-staged": "^11.0.0",
    "ng-packagr": "^12.0.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.2.4"
  }
}
